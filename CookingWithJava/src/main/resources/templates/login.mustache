        <h2>Register / Log In</h2>
    </div>
</section>
<!-- Hero section end -->


<section class="contact-section spad">
    <div class="container">
        <div class="row">
            <nav class="col-lg-8">
                <div class="nav nav-tabs nav-fill col-md-12" id="nav-tab" role="tablist">
                    <a class="col-md-6 nav-item nav-link active" id="nav-register-tab" data-toggle="tab"
                       href="#nav-register"
                       role="tab"
                       aria-controls="nav-register" aria-selected="true">Register</a>
                    <a class="col-md-6 nav-item nav-link" id="nav-login-tab" data-toggle="tab" href="#nav-login"
                       role="tab"
                       aria-controls="nav-login" aria-selected="false">Login</a>
                </div>
            </nav>
        </div>
        <div class="row">
            <div class="col-lg-8">
                <div class="contact-form-warp">
                    <div class="tab-content py-3 px-3 px-sm-0" id="nav-tabContent">
                        <div class="tab-pane fade show active" id="nav-register" role="tabpanel"
                             aria-labelledby="nav-register-tab">
                            <form class="contact-form" method="post">
                                <div>
                                    <div class="form-group row col-md-12">
                                        <label class="col-sm-3" for="nickRegistro">Nick:</label>
                                        <input class="col-sm-9" id="nickRegistro" name="nickRegistro" type="text"
                                               placeholder="Nick nuevo usuario">
                                    </div>
                                    <div class="form-group row col-md-12">
                                        <label class="col-sm-3" for="correoRegistro">Correo electrónico:</label>
                                        <input class="col-sm-9" id="correoRegistro" name="correoRegistro" type="text"
                                               placeholder="Correo electrónico nuevo usuario">
                                    </div>
                                    <div class="form-group row col-md-12">
                                        <label class="col-sm-3" for="nombreRegistro">Nombre y apellidos:</label>
                                        <input class="col-sm-9" id="nombreRegistro" name="nombreRegistro" type="text"
                                               placeholder="Nombre y apellidos nuevo usuario">
                                    </div>
                                    <div class="form-group row col-md-12">
                                        <label class="col-sm-3" for="contrasenaRegistro">Contraseña:</label>
                                        <input class="col-sm-9" id="contrasenaRegistro" name="contrasenaRegistro"
                                               type="password"
                                               placeholder="*****">
                                    </div>
                                    <div class="form-group row col-md-12">
                                        <label class="col-sm-3" for="contrasena2Registro">Repetir contraseña:</label>
                                        <input class="col-sm-9" id="contrasena2Registro" name="contrasena2Registro"
                                               type="password"
                                               placeholder="*****">
                                    </div>
                                    <button type="button" class="site-btn col-md-12" id="buttonUserRegister">Registrar
                                    </button>
                                </div>
                            </form>
                        </div>
                        <div class="tab-pane fade" id="nav-login" role="tabpanel"
                             aria-labelledby="nav-login-tab">
                            <form class="contact-form">
                                <div>
                                    <div class="form-group row col-md-12">
                                        <label class="col-sm-3" for="nickLogin">Nick:</label>
                                        <input class="col-sm-9" id="nickLogin" name="nickLogin" type="text"
                                               placeholder="Nick nuevo usuario">
                                    </div>
                                    <div class="form-group row col-md-12">
                                        <label class="col-sm-3" for="contrasenaLogin">Contraseña:</label>
                                        <input class="col-sm-9" id="contrasenaLogin" name="contrasenaLogin"
                                               type="password"
                                               placeholder="*****">
                                    </div>
                                    <button type="button" class="site-btn col-md-12" id="buttonUserLogin">Login</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="contact-logo">
                    <img src="img/logo.png" alt="">
                </div>
            </div>
        </div>
        <!-- map -->

    </div>
</section>

